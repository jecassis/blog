{
  "rewrites": [
    { "source": "/pgp", "destination": "/api/pgp" },
    { "source": "/atom", "destination": "/api/atom" },
    { "source": "/views", "destination": "/api/views" }
  ],
  "functions": {
    "api/atom.ts": {
      "includeFiles": "posts/**"
    },
    "api/pgp.ts": {
      "includeFiles": "public/pgp.asc"
    }
  }
}
